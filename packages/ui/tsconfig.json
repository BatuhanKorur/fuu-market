{
  "extends": "@fuu/tsconfig/react.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": [
    "src/components/**/*.ts",
    "src/components/**/*.tsx",
    "src/lib/**/*.ts"
  ],
  "exclude": ["node_modules"],
  "jsxImportSource": "react"
}
